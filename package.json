	"scripts": {
		"dev": "webpack server --config webpack/webpack.dev.js --env build=dev",
		"base": "webpack server --config webpack/webpack.dev.js --env build=base",
		"stag": "webpack server --config webpack/webpack.dev.js --env build=stag",
		"prod": "webpack server --config webpack/webpack.dev.js --env build=prod",
